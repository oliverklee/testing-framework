{
    "name": "nimut/testing-framework",
    "description": "TYPO3 testing framework that provides base classes and configuration for PHPUnit tests",
    "keywords": [
        "TYPO3 CMS",
        "PHPUnit",
        "testing"
    ],
    "homepage": "https://github.com/Nimut/TYPO3-testing-framework",
    "license": [
        "GPL-2.0+"
    ],
    "authors": [
        {
            "name": "Nicole Cordes",
            "email": "typo3@cordes.co",
            "role": "Developer"
        },
        {
            "name": "Helmut Hummel",
            "email": "info@helhum.io",
            "role": "Developer",
            "homepage": "http://helhum.io"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "compat/Packages/class-alias"
        },
        {
            "type": "path",
            "url": "tests/Packages/testbase"
        }
    ],
    "require": {
        "php": "< 7.3",
        "typo3/cms-core": ">=7.6 <9.1 || dev-master",
        "typo3/cms-extbase": ">=7.6 <9.1 || dev-master",
        "typo3/cms-fluid": ">=7.6 <9.1 || dev-master",
        "typo3/cms-frontend": ">=7.6 <9.1 || dev-master",
        "typo3/cms-install": ">=7.6 <9.1 || dev-master",
        "phpunit/phpunit": "^6.0",
        "mikey179/vfsStream": "^1.6"
    },
    "require-dev": {
        "nimut/testing-framework-testbase": "@dev",
        "nimut/phpunit-merger": "^0.1"
    },
    "suggest": {
        "friendsofphp/php-cs-fixer": "Tool to automatically fix PHP coding standards issues"
    },
    "autoload": {
        "psr-4": {
            "Nimut\\TestingFramework\\": "src/TestingFramework/",
            "Nimut\\TestingFramework\\v76\\": "compat/v76/",
            "Nimut\\TestingFramework\\v87\\": "compat/v87/"
        }
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        },
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/Web"
        }
    },
    "include-path": [
        ".Build/vendor/pear/http_request2/",
        ".Build/vendor/pear/net_url2/"
    ]
}
